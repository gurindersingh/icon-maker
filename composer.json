{
    "name": "gurindersingh/icon-maker",
    "description": "Icon maker for Laravel webapp",
    "keywords": [
        "laravel",
        "laravel zero",
        "console",
        "cli",
        "Icon maker",
        "Icons"
    ],
    "homepage": "https://github.com/gurindersingh/icon-maker",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/gurindersingh/icon-maker/issues",
        "source": "https://github.com/gurindersingh/icon-maker"
    },
    "authors": [
        {
            "name": "Gurinder Chauhan",
            "email": "me@gurinder.ca"
        }
    ],
    "require": {
        "php": "^8.1",
        "intervention/image": "^2.7",
        "laravel-zero/framework": "^9.2",
        "nunomaduro/termwind": "^1.15"
    },
    "require-dev": {
        "laravel/pint": "^1.2",
        "mockery/mockery": "^1.4.4",
        "pestphp/pest": "^1.21.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": [
        "builds/icon-maker"
    ]
}
